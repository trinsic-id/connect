{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SpaProxy": {
    "Routes": {
      "viewRoute1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "Host",
            "Set": "localhost:4012"
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "react": {
            "Address": "http://localhost:4012/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": "true"
        }
      }
    }
  }
}
