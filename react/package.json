{
  "name": "@trinsic-connect/demos",
  "private": true,
  "main": "src/main.tsx",
  "scripts": {
    "build-pocketride": "tsc && RELYING_PARTY='pocketride' vite build",
    "build-pearbnb": "tsc && RELYING_PARTY='pearbnb' vite build",
    "build": "npm run build-pocketride && npm run build-pearbnb",
    "build:post": "sh ./post-build.sh",
    "build:eslint": "npx eslint src",
    "build:prettier": "prettier src --write",
    "find-unimported": "npx unimported",
    "start-pocketride": "cross-env RELYING_PARTY='pocketride' concurrently \"vite --host\" \"dotnet run --project ../pocketride/service/PocketRide.csproj\"",
    "start-pearbnb": "cross-env RELYING_PARTY='pearbnb' concurrently \"vite --host\" \"dotnet run --project ../pearbnb/service/PearBnB.csproj\""
  },
  "dependencies": {
    "@trinsic/trinsic": "1.13.1-rc1",
    "framer-motion": "10.15.0",
    "lodash": "4.17.21",
    "long": "5.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-query": "3.39.3",
    "react-router-dom": "6.14.2",
    "react-spinkit": "3.0.0",
    "react-use": "17.4.0",
    "recoil": "0.7.7",
    "tailwindcss-animated": "1.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.4",
    "@types/lodash": "4.14.196",
    "@types/micromodal": "^0.3.3",
    "@types/node": "^20.5.7",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/react-spinkit": "3.0.7",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-basic-ssl": "1.0.1",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.2.1",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "eslint": "8.46.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "postcss": "^8.4.32",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "prettier-plugin-tailwindcss": "0.4.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "unimported": "1.29.2",
    "vite": "^4.5.1",
    "vite-plugin-checker": "0.6.1",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0"
  }
}
